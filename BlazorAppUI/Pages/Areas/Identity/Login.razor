@page "/"
@using BlazorAppUI.Contracts.Identity
@using BlazorUI.Applications

<EditForm Model="model" OnSubmit="OnSubmit">
    <input @bind-value="@model.UserName" />
    <input @bind-value="@model.Password" />
    <input type="submit" value="Submit" />
</EditForm>
@code {
    [Inject] AuthenticationService service { get; set; }
    LoginDto model = new();

    private async Task OnSubmit()
    {
        await service.Login(model.UserName, model.Password);
    }

}
